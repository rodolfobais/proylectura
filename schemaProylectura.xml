            <reference local="id_usuario" foreign="id"/>
        </foreign-key>
        <foreign-key foreignTable="usuario" name="amistad_id_usuarioamigo">
            <reference local="id_usuarioamigo" foreign="id"/>
        </foreign-key>
    </table>

